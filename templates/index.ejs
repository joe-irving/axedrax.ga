<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <title>Axe Drax</title>
    <%- head; %>
  </head>
  <body>
    <header>
      <%- header; %>
    </header>
    <nav>
      <ul class = 'menu'>
      <% for (url in navconfig){ %>
        <%- "<li>" %>
        <% if (typeof navconfig[url] === 'object'){ %>
          <%- `<a href="" onclick="return false;">${navconfig[url].title}</a>` %>
          <%- "<ul class='submenu'>" %>
          <% for (subUrl in navconfig[url].values){ %>
            <%- `<li><a href="${subUrl}">${navconfig[url].values[subUrl]}</a></li>` %>
          <% }; %>
          <%- "</ul>" %>
        <% }else{ %>
          <%- `<a href="${url}">${navconfig[url]}</a>` %>
        <% }; %>
        <%- "</li>" %>
      <% }; %>
      </ul>
    </nav>
    <div class="main background">
      <main>
          <article>
            <%- main; %>
          </article>
      </main>
    </div>
    <footer>
      <%- footer; %>
    </footer>
  </body>
</html>
